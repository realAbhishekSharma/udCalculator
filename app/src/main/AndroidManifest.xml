<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.realhydrogen.uniquedesktopcalulator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/unique_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/unique_logo"
        android:supportsRtl="true">
        <activity android:name=".StickerCostSingleColor"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".StickerCostFourColor"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".PaperCost"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".BillBookMiniOffset"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".SingleColorDoubleSide"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".SingleColorSingleSide"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".FourColorDoubleSide"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".BillSelection"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SideSelection"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrontList"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FourColorSingleSideActivity"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppThemeNoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>